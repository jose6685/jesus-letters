{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/(assets/.*|icons/.*|sw\\.js|manifest\\.json)",
      "dest": "/$1"
    },
    {
      "src": "/((?!assets/|icons/|sw\\.js|manifest\\.json).*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}