{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(assets/.*|icons/.*|sw\\.js|manifest\\.json)",
      "dest": "/$1"
    },
    {
      "src": "/((?!assets/|icons/|sw\\.js|manifest\\.json).*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}